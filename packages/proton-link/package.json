{
  "name": "@protonprotocol/proton-link",
  "author": "jafri",
  "module": "dist/index.js",
  "unpkg": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "version": "2.5.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "watch": "tsdx watch  --verbose --noClean",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/proton-link.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/proton-link.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@protonprotocol/proton-signing-request": "^2.4.7",
    "@protonprotocol/protonjs": "^21.0.21",
    "eosjs-ecc": "^4.0.7",
    "isomorphic-ws": "^4.0.1",
    "pako": "^1.0.11",
    "uuid": "^8.3.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/pako": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.7"
  }
}
